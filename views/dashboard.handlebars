{{#if posts.length}}
<section>
  <h2 class="header-page">Your Posts</h2>
    {{#each posts as |post|}}
      {{> dashboardPost post}}
    {{/each}}

</section>
{{/if}}

<button type="submit" class="btn"><a class="link-button new-form" href="/dashboard/new">+ New Post</a></button>

<script src="/js/new.js"></script>

